<!--Widok czesciowy, mozna sie do niego odwołac-->
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="ListaKategoriZawartosc">
    @Html.Action("KategorieMenu","Produkty")

    <div id="KategorieListaPrawaSekcja">
        <div id="Breadcrumb">
            <img src="../Content/Images/dommaly.png" />
            @Html.MvcSiteMap().SiteMapPath()
        </div>
        <section class="SekcjaKoszyk">
            <div class="SekcjaHeader" style="margin-bottom:0px;">
                <h1>
                    @RenderSection("SectionTitle",required:false)
                </h1>
            </div>
            @RenderBody()
        </section>
    </div>
</div>
@section Scripts
{
    @RenderSection("Scripts", required: false) 
}